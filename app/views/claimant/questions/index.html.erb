<p style="color: green"><%= notice %></p>

<% content_for :title, "Questions" %>

<h1>Questions</h1>

<div id="questions">
  <% @questions.each do |question| %>
    <%= render question %>
    <p>
      <%= link_to "Show this question", claimant_question_path(question) %>
    </p>
  <% end %>
</div>

<%= link_to "New question", new_claimant_question_path %>
